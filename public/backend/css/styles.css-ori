.additional-info .extra-card {position: relative;border: 1px solid #175890;text-align: center;padding: 8px 12px 40px;margin-bottom: 12px;font-size: 14px;line-height: 1.715;min-height: 340px;word-break: break-word;word-wrap: break-word;-webkit-hyphens: auto;-ms-hyphens: auto;hyphens: auto;}
