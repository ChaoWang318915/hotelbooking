/* extra-menu */
.extra-menu {border-top: 32px solid #175890;background: #fbfcff;font-size: 16px;line-height: 1;word-break: break-word;word-wrap: break-word;-webkit-hyphens: auto;-ms-hyphens: auto;hyphens: auto;}
