.table-responsive {width: 100%;overflow-x: auto;-webkit-overflow-scrolling: touch;margin-bottom: 54px;}
.table-responsive .table {margin-bottom: 0;}
.table {width: 100%;border-collapse: collapse;margin-bottom: 54px;font-size: 14px;line-height: 1.5;border: 1px solid #e9e9e9;color: #666;}
